<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ATM</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
	
	<servlet>
        <servlet-name>GetStatusServlet</servlet-name>
        <servlet-class>servlet.GetStatusServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TurnOnServlet</servlet-name>
        <servlet-class>servlet.TurnOnServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TurnOffServlet</servlet-name>
        <servlet-class>servlet.TurnOffServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CardInsertedServlet</servlet-name>
        <servlet-class>servlet.CardInsertedServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AuthServlet</servlet-name>
        <servlet-class>servlet.AuthServlet</servlet-class>
    </servlet>    
    <servlet>
        <servlet-name>TransactionServlet</servlet-name>
        <servlet-class>servlet.TransactionServlet</servlet-class>
    </servlet> 
	<servlet>
        <servlet-name>WithdrawInfoServlet</servlet-name>
        <servlet-class>servlet.WithdrawInfoServlet</servlet-class>
    </servlet> 
   	<servlet>
        <servlet-name>WithdrawPrintServlet</servlet-name>
        <servlet-class>servlet.WithdrawPrintServlet</servlet-class>
    </servlet>     
     <!--存款的servlet-->
    <servlet>
        <servlet-name>DepositServlet</servlet-name>
        <servlet-class>servlet.DepositServlet</servlet-class>
    </servlet> 
   	<servlet>
        <servlet-name>DepositPrintServlet</servlet-name>
        <servlet-class>servlet.DepositPrintServlet</servlet-class>
    </servlet>   
    
    <servlet-mapping>
        <servlet-name>GetStatusServlet</servlet-name>
        <url-pattern>/GetStatusServlet</url-pattern>
    </servlet-mapping> 
  	<servlet-mapping>
        <servlet-name>TurnOnServlet</servlet-name>
        <url-pattern>/TurnOnServlet</url-pattern>
    </servlet-mapping> 
    <servlet-mapping>
        <servlet-name>TurnOffServlet</servlet-name>
        <url-pattern>/TurnOffServlet</url-pattern>
    </servlet-mapping> 
    <servlet-mapping>
        <servlet-name>CardInsertedServlet</servlet-name>
        <url-pattern>/CardInsertedServlet</url-pattern>
    </servlet-mapping> 
	<servlet-mapping>
        <servlet-name>AuthServlet</servlet-name>
        <url-pattern>/AuthServlet</url-pattern>
    </servlet-mapping> 
    <servlet-mapping>
        <servlet-name>WithdrawInfoServlet</servlet-name>
        <url-pattern>/WithdrawInfoServlet</url-pattern>
    </servlet-mapping> 
    <servlet-mapping>
        <servlet-name>WithdrawPrintServlet</servlet-name>
        <url-pattern>/WithdrawPrintServlet</url-pattern>
    </servlet-mapping> 
    <servlet-mapping>
        <servlet-name>TransactionServlet</servlet-name>
        <url-pattern>/TransactionServlet</url-pattern>
    </servlet-mapping> 
    <!--存款的servlet-->
     <servlet-mapping>
        <servlet-name>DepositServlet</servlet-name>
        <url-pattern>/DepositServlet</url-pattern>
    </servlet-mapping> 
    <servlet-mapping>
        <servlet-name>DepositPrintServlet</servlet-name>
        <url-pattern>/DepositPrintServlet</url-pattern>
    </servlet-mapping> 
    
</web-app>